<div class="cards--squares">
  <h2 class="cards__title"></h2>
  
  <div class="cards__cards">
    <% cards.each do |card| %>
      <div class="card">
        <%= link_to card[:url], title: '', class: 'card__link' do %>
          <h3 class="card__title"><%= card[:title] %></h3>
        <% end %>
      </div>
    <% end %>
  </div>
</div>