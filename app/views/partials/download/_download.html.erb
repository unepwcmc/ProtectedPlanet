<download 
  button-text="<%= t('global.button.download') %>"

  <% if local_assigns.has_key? :classes %>
    class="<%= classes %>"
  <% end %>

  <% if local_assigns.has_key? :download_disabled %>
  :download-disabled="<%= download_disabled %>"
  <% end %>
  
  ga-id="download"
  :options="<%= options %>"
  :text-commercial="<%= download_text[:commercial].to_json %>"
>
</download>