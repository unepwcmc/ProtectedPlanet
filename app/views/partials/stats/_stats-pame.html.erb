<div class="card--stats">
  <h2 >Management Effectiveness Evaluations</h2>

  <% if pame_evaluations_summary.any? %>
    <% pame_evaluations_summary.each do |methodology, years| %>
      <p class="attribute">
        <span class="attribute__title"><%= methodology %></span>
        <span class="attribute__value"><%= years.join(", ") %></span>
      </p>
    <% end %>
  <% else %>
    <p>No information available</p>
  <% end %>
</div>