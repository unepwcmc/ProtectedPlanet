<section class="container section-padding bg--grey-xlight">
  <div class="container--medium">
    <h2><%= cms_fragment_render(:section_ocean_title, @cms_page) %></h2>
    <div><%= cms_fragment_render(:section_ocean_content, @cms_page) %></div>
    <div>
      <h3>National waters</h3>
      <chart-row-pa percent="<%= national %>" coverage="<%= national_coverage %>" ></chart-row-pa>

      <h3>High seas</h3>
      <chart-row-pa percent="<%= high_seas %>" coverage="<%= high_seas_coverage %>" ></chart-row-pa>
      <div class="chart__legend-item">
        <span class="chart__legend-key"></span>
        <span class="chart__legend-text">% of Protected Area Coverage</span>
      </div>
    </div>
  </div>
</section>
