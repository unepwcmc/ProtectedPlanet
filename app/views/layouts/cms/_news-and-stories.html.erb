<%
results = {
  title: 'Title', 
  total: 1, 
  cards: [
    { date: '02 December 2019', image: 'image', summary: 'summary', title: 'title', url: 'http://google.com' }
  ]
}.to_json
%>

<div class="page--news">
  <%= render partial: "./layouts/partials/hero-small", locals: { 
    classes: @cms_page[:slug],
    hero_image: cms_fragment_render(:hero_image, @cms_page),
    title: @cms_page.label
  } %>

  <listing-page
    endpoint-search="/en/search-cms"
    :filter-groups="<%= get_category_filters %>"
    :page-id="<%= @cms_page.id %>"
    :results="<%= results %>"
    sm-trigger-element="sm-listing"
    template="news"
    text-filters-close="View Results"
    text-filter-trigger="Filters"
    text-no-results="No Results"
  ></listing-page>

  <%= render "partials/ctas/live-report" %>
</div>